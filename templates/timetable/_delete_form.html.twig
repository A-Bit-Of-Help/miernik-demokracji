<form method="post" action="{{ path('timetable_delete', {'id': timetable.id}) }}"
      onsubmit="return confirm('{% trans %}timetable.delete.confirm{% endtrans %}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ timetable.id) }}">
    <button class="btn btn-danger">{% trans %}timetable.delete.button{% endtrans %}</button>
</form>
