<form method="post" action="{{ path('votes_delete', {'id': vote.id}) }}"
      onsubmit="return confirm('{% trans %}votes.delete.confirm{% endtrans %}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vote.id) }}">
    <button class="btn btn-danger">{% trans %}votes.delete.button{% endtrans %}</button>
</form>
